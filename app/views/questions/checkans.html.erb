<div class>
	<div class="row">
		<!-- <div class="jumbotron"> -->	
    		<div class="col-lg-3">
    			
    		</div>
    	<!-- </div> -->
    	<form>
  <div class="form-group">
    <div class="col-lg-5">

			<h3>Please Answer Questionz</h3>
			<% @user = User.find(current_user.id) %>
 			<% if Question.exists?(:user_id => @user,  level: !nil) %>
      			<% @level = Question.where(user_id: @user).last.level %>
    		<% else %>
     			<% @level = 1 %>
   			<% end %>
   			<p><%= @user.id %> </p>
			<p><%= @level %> </p>
			<p><%= CheckNum.questionGen(@level) %> </p>
			
			<%= form_tag "/validate" do %>
			<%= text_field_tag :search_string %>
			<br />
			<%= submit_tag "Search" %>
		<% end %>
	</div>
	</div>
</form>

	</div>
</div>